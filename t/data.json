[
	{
		"templates": [
			"http://{authority}/"
		],
		"uris": {
			"http://example.com/": { "authority": "example.com" },
			"http://example.org/": { "authority": "example.org" },
			"http://www.example.net/": { "authority": "www.example.net" }
		}
	},
	{
		"templates": [
			"http:{+hierpart}"
		],
		"uris": {
			"http://example.com/": { "hierpart": "//example.com/" },
			"http://example.org/": { "hierpart": "//example.org/" },
			"http://www.example.net/": { "hierpart": "//www.example.net/" }
		}
	},
	{
		"templates": [
			"http://localhost/{document_any}",
			"http://localhost/{document_txt}.txt",
			"http://localhost/{document_html}.html",
			"http://localhost/{document_json}.json"
		],
		"uris": {
			"http://localhost/about": { "document_any": "about" },
			"http://localhost/about.txt": { "document_txt": "about" },
			"http://localhost/about.html": { "document_html": "about" },
			"http://localhost/about.json": { "document_json": "about" }
		}
	},
	{
		"templates": [
			"/a/{any}",
			"/a/{txt}.txt"
		],
		"uris": {
			"/a/pg": { "any": "pg" },
			"/a/pg.txt": { "txt": "pg" }
		}
	},
	{
		"templates": [
			"/b/{txt}.txt",
			"/b/{any}"
		],
		"uris": {
			"/b/pg": { "any": "pg" },
			"/b/pg.txt": { "txt": "pg" }
		}
	},
	{
		"templates": [
			"http://a.localhost{/path*}"
		],
		"uris": {
			"http://a.localhost": {},
			"http://a.localhost/": { "path": [ "" ] },
			"http://a.localhost/a": { "path": [ "a" ] },
			"http://a.localhost/a/b/c/d/e/f/g": { "path": [ "a", "b", "c", "d", "e", "f", "g" ] }
		}
	},
	{
		"templates": [
			"http://b.localhost/",
			"http://b.localhost{/path*}"
		],
		"uris": {
			"http://b.localhost": {},
			"http://b.localhost/": { "path": [""] },
			"http://b.localhost/a": { "path": [ "a" ] }
		}
	},
	{
		"templates": [
			"http://c.localhost{/path*}{?query*}"
		],
		"uris": {
			"http://c.localhost": {},
			"http://c.localhost/": { "path": [""] },
			"http://c.localhost/a": { "path": [ "a" ] },
			"http://c.localhost/a?x=y": { "path": [ "a" ], "query": ["x=y"] }
		}
	},
	{
		"templates": [
			"http://example.com/",
			"http://example.com/root{/base}{/a*}{?b*}{#c}",
			"http://example.com/blog{/y,m,d,slug}"
		],
		"uris": {
			"http://example.com/": {},
			"http://example.com/root": { },
			"http://example.com/root/base/b/c": { "base":"base", "a": [ "b", "c" ] },
			"http://example.com/root/base/b/c?d&e": { "base":"base", "a": [ "b", "c" ], "b":[ "d", "e" ] }
		}
	}
]
